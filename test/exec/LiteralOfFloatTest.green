@Export
int main() {
    assert(3.14 >= 2.00);
    assert(0.0 > -1.0);
    assert(12345.0 > 12340.0);
    assert(1e10 > 1000000000  /*1e9*/);
    assert(1e10 < 100000000000/*1e11*/);
    assert((1.1111111111111111e20 - 111111111111111111111.0  ) < 0.01);
    assert((1.1111111111111111e21 - 1111111111111111111111.0 ) < 0.01);
    assert((1.1111111111111111e22 - 11111111111111111111111.0) < 0.01);
    assert((1e-5 - 0.00001  ) < 0.01);
    assert((1e-6 - 0.000001 ) < 0.01);
    assert((1e-7 - 0.0000001) < 0.01);
    assert(0.1  == 0.1);
    assert(0.01 == 0.01);
    assert(1.0  == 1.0);
    assert(10.0 == 10.0);
    return 0;
}

