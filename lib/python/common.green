
// libgreentea
@Common void print(any x)   as "print $1";
@Common void println(any x) as "print $1";
@Common void assert(boolean x) as "assert($1)";

// top type
/* @Common @Const @Operator int "||"(Top x) import Konoha.TopApi.Size; */
@Common @Const @Operator boolean "=="(Top x, Top y) as "$1 == $2";
@Common @Const @Operator boolean "!="(Top x, Top y) as "$1 != $2";

