// AnyApi
@Common @Coercion @Const boolean converter(any x) as "bool($1)";
@Common @Coercion @Const int converter(any x) as "int($1)";
@Common @Coercion @Const float converter(any x) as "float($1)";

// StringApi
@Common @Const @Operator String "+"(String x, any y) as "$1 + str($2)";
//@Const @Operator String "+"(int x, String y) import GreenTeaRuntime.Add;
//@Const @Operator String "+"(boolean x, String y) import GreenTeaRuntime.Add;

@Common @Const @Operator boolean "=="(String x, String y) as "$1 == $2";
@Common @Const @Operator boolean "!="(String x, String y) as "$1 != $2";

@Common @Const @Operator int "||"(String x) as "len($1)";
@Common @Const @Operator String "[]"(String x, int index) as "$1[$2]";
@Common @Const @Operator String "[:]"(String x, int beginIndex) as "$1[$2:]";
@Common @Const @Operator String "[:]"(String x, int beginIndex, int endIndex) as "$1[$2:$3]";
/* @Common @Const @Operator Iterator<String> ".."(String x) import Konoha.StringApi.ToIterator; */

@Common @Const boolean startsWith(String x, String y) as "$1.startswith($2)";
@Common @Const boolean endsWith(String x, String y) as "$1.endswith($2)";
@Common @Const int indexOf(String x, String y) as "$1.find($2)";
@Common @Const int lastIndexOf(String x, String y) as "$1.rfind($2)";

@Const var split(String x, String y) as "$1.split($2)";

//@Common @Const @Coercion any converter(String x) import Konoha.StringApi.ToAny;
@Common @Const int converter(String x) as "int($1)";
@Common @Const float converter(String x) as "float($1)";

/* @Hidden @Coercion @Const var converter(var x) import Konoha.StringApi.s2c; */
/* @Hidden @StrongCoercion @Const var converter(var x) import Konoha.StringApi.c2s; */

// BooleanApi
@Common @Const @Operator boolean not(boolean x) as "not $1";
@Common @Const @Operator boolean "!"(boolean x) as "not $1";
@Common @Const @Operator boolean "=="(boolean x, boolean y) as "$1 == $2";
@Common @Const @Operator boolean "!="(boolean x, boolean y) as "$1 != $2";

@Common @Const @Coercion String converter(boolean x) as "str($1)";
/* @Common @Const @Coercion any converter(boolean x) import Konoha.BooleanApi.ToAny; */

// IntApi
@Common @Const @Operator int "+"(int x) as "+$1";
@Common @Const @Operator int "-"(int x) as "-$1";
@Common @Const @Operator int "~"(int x) as "~$1";

@Common @Const @Operator int "+"(int x, int y) as "$1 + $2";
@Common @Const @Operator int "-"(int x, int y) as "$1 - $2";
@Common @Const @Operator int "*"(int x, int y) as "$1 * $2";
@Common @Const @Operator int "/"(int x, int y) as "$1 / $2";
@Common @Const @Operator int "%"(int x, int y) as "$1 % $2";
@Common @Const @Operator int "<<"(int x, int y) as "$1 << $2";
@Common @Const @Operator int ">>"(int x, int y) as "$1 >> $2";
@Common @Const @Operator int "^"(int x, int y) as "$1 ^ $2";
@Common @Const @Operator int "|"(int x, int y) as "$1 | $2";
@Common @Const @Operator int "&"(int x, int y) as "$1 & $2";
@Common @Const @Operator boolean "<"(int x,  int y) as "$1 < $2";
@Common @Const @Operator boolean "<="(int x, int y) as "$1 > $2";
@Common @Const @Operator boolean ">"(int x,  int y) as "$1 <= $2";
@Common @Const @Operator boolean ">="(int x, int y) as "$1 >= $2";
@Common @Const @Operator boolean "=="(int x, int y) as "$1 == $2";
@Common @Const @Operator boolean "!="(int x, int y) as "$1 != $2";

@Common @Const @Operator float "+"(int x, float y) as "$1 + $2";
@Common @Const @Operator float "-"(int x, float y) as "$1 - $2";
@Common @Const @Operator float "*"(int x, float y) as "$1 * $2";
@Common @Const @Operator float "/"(int x, float y) as "$1 / $2";
@Common @Const @Operator float "%"(int x, float y) as "$1 % $2";

/* @Coercion @Const any converter(int x) import Konoha.IntApi.ToAny; */
@Common @Coercion @Const String converter(int x) as "str($1)";
@Common @Coercion @Const float converter(int x) as "float($1)";

