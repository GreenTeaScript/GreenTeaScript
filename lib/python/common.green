// ArrayApi
@Common @Const @Operator <T> int "||"(Array<T> this) as "len($1)";
@Common @Const @Operator <T> T "[]"(Array<T> this, int index) as "$1[$2]";
@Common @Operator <T> void "[]="(Array<T> this, int index, T value) as "$1[$2] = $3";
@Common <T> Array<T> add(Array<T> this, T value) as "$1.append($2)";
@Common @Const @Operator <T> Array<T> "[:]"(Array<T> x, int beginIndex) as "$1[$2:]";
@Common @Const @Operator <T> Array<T> "[:]"(Array<T> x, int beginIndex, int endIndex) as "$1[$2:$3]";

