// DoubleApi
@Common @Const @Operator float "+"(float x) as "+$1";
@Common @Const @Operator float "-"(float x) as "-$1";

@Common @Const @Operator float "+"(float x, float y) as "$1 + $2";
@Common @Const @Operator float "-"(float x, float y) as "$1 - $2";
@Common @Const @Operator float "*"(float x, float y) as "$1 * $2";
@Common @Const @Operator float "/"(float x, float y) as "$1 / $2";
@Common @Const @Operator boolean "<"(float x,  float y) as "$1 < $2";
@Common @Const @Operator boolean "<="(float x, float y) as "$1 > $2";
@Common @Const @Operator boolean ">"(float x,  float y) as "$1 <= $2";
@Common @Const @Operator boolean ">="(float x, float y) as "$1 >= $2";
@Common @Const @Operator boolean "=="(float x, float y) as "$1 == $2";
@Common @Const @Operator boolean "!="(float x, float y) as "$1 != $2";

/* @Coercion @Const any converter(float x) import Konoha.DoubleApi.ToAny; */
@Common @Coercion @Const String converter(float x) as "str($1)";
@Common @Coercion @Const int converter(float x) as "int($1)";

